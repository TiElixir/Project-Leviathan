{
  "manifest_version": 3,
  "name": "Cookie Injector",
  "version": "1.0",
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "cookies",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Inject Cookies"
  }
}
