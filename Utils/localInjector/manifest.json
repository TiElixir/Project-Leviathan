{
  "manifest_version": 3,
  "name": "Injector local",
  "version": "1.0",
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Inject LocalStorage"
  }
}
